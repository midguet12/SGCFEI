<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="450.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sgcfei.menus.Administrador.Catalogos.LGCA.RegistrosController">
  <children>
    <VBox alignment="CENTER" prefHeight="300.0" prefWidth="450.0" spacing="20.0">
      <children>
        <Label />
        <TableView fx:id="tabla" maxHeight="200.0" maxWidth="390.0" minHeight="200.0" minWidth="390.0" onMouseClicked="#clickTabla" prefHeight="200.0" prefWidth="390.0">
          <columns>
            <TableColumn prefWidth="100.0" text="Clave" fx:id="cClave" />
            <TableColumn prefWidth="285.0" text="Experiencia Educativa" fx:id="cDescripcion" />
          </columns>
        </TableView>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
          <children>
            <Button fx:id="btnAgregar" maxWidth="70.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#clickAgregar" prefWidth="70.0" text="Agregar" />
            <Button fx:id="btnConsultar" maxWidth="70.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#clickConsultar" prefWidth="70.0" text="Consultar" />
            <Button fx:id="btnEditar" maxWidth="70.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#clickEditar" prefWidth="70.0" text="Editar" />
            <Button fx:id="btnEliminar" maxWidth="70.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#clickEliminar" prefWidth="70.0" text="Eliminar" />
            <Button fx:id="btnCancelar" maxWidth="70.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#clickCancelar" prefWidth="70.0" text="Cancelar" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
