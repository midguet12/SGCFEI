<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
    <children>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0" spacing="20.0">
         <children>
            <Label text="NRC:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblNrc" text="nrc" />
            <Label text="Nombre:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblNombre" text="nombre" />
            <Label text="Academia:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblAcademia" text="academia" />
            <Button fx:id="btnAceptar" mnemonicParsing="false" onMouseClicked="#aceptar" text="Aceptar" />
         </children>
      </VBox>
   </children>
</AnchorPane>
