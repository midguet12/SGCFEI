<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="380.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <VBox alignment="CENTER" prefHeight="380.0" prefWidth="400.0" spacing="20.0">
         <children>
            <Label text="Numero de personal:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="tfNumeroPersonal" maxWidth="380.0" minWidth="380.0" prefWidth="380.0" />
            <Label text="Nombre:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="tfNombre" maxWidth="380.0" minWidth="380.0" prefWidth="380.0" />
            <Label text="Correo:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="tfCorreo" maxWidth="380.0" minWidth="380.0" prefWidth="380.0" />
            <Label text="Rol:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ComboBox fx:id="cboxRol" maxWidth="380.0" minWidth="380.0" prefWidth="380.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button fx:id="btnAceptar" mnemonicParsing="false" onMouseClicked="#aceptar" text="Aceptar" />
                  <Button fx:id="btnCancelar" mnemonicParsing="false" onMouseClicked="#cancelar" text="Cancelar" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
