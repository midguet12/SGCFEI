<AnchorPane id="AnchorPane" prefHeight="492.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="126.0" layoutY="14.0" text="Editar Usuario">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="12.0" layoutY="79.0" text="Numero personal" />
      <Label layoutX="14.0" layoutY="141.0" text="Nombre" />
      <Label layoutX="14.0" layoutY="211.0" text="Correo electronico" />
      <TextField fx:id="tfNumeroPersonal" layoutX="14.0" layoutY="107.0" prefHeight="25.0" prefWidth="364.0" />
      <TextField fx:id="tfNombre" layoutX="14.0" layoutY="172.0" prefHeight="25.0" prefWidth="364.0" />
      <TextField fx:id="tfCorreo" layoutX="14.0" layoutY="240.0" prefHeight="25.0" prefWidth="364.0" />
      <Label layoutX="17.0" layoutY="377.0" text="Tipo de Usuario" />
      <ComboBox fx:id="cbTipoUsuario" layoutX="19.0" layoutY="405.0" prefHeight="25.0" prefWidth="364.0" />
