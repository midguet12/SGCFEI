<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="8" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sgcfei.menus.LoginController">
    <TextField fx:id="username"  onKeyPressed="#loginPorIntro" maxWidth="200" promptText="Usuario" />
    <PasswordField fx:id="password"  onKeyPressed="#loginPorIntro" maxWidth="200" promptText="Contraseña" />
    <Label fx:id="alertLabel" />
    <Button fx:id="button" onMouseClicked="#loginPorClick" text="Iniciar sesión"/>
</VBox>

